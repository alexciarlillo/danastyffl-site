    "webpack": "^3.11"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "browser-sync": "^2.24.0",
    "browser-sync-webpack-plugin": "^2.2.0",
    "font-awesome": "^4.7.0",
    "popper.js": "^1.14",
    "vue-router": "^3.0",
    "vue-simple-loading": "^1.1.5",
